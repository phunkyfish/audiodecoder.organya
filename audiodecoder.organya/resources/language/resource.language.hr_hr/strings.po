msgctxt "#30000"
msgid "Loop indefinitely"
msgstr "Ponavljaj zauvijek"

msgctxt "#30001"
msgid "When enabled, plays either infinite or repeats with the following setting."
msgstr "Kada je omogućeno, reproducira se beskonačno ili se ponavlja sa sljedećim postavkama."

msgctxt "#30002"
msgid "Loop count"
msgstr "Broj ponavljanja"

msgctxt "#30003"
msgid "The number of repetitions or as infinite."
msgstr "Broj ponavljanja ili kao beskonačno."

msgctxt "#30004"
msgid "Infinity"
msgstr "Beskonačno"

msgctxt "#30005"
msgid "Fade time (ms)"
msgstr "Vrijeme izbljeđivanja (ms)"

msgctxt "#30006"
msgid "Time at the end to reduce the volume."
msgstr "Vrijeme na kraju za smanjenje glasnoće zvuka."
